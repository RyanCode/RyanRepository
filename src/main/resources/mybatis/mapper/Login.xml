<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ryan.springbootvue.mapper.UserMapper">
    <!--List<User> findList(User user);
    int insert(User user);    List<User> findUser(User user);-->
    <select id="findList" resultType="com.ryan.springbootvue.entity.User">
        select * from user
    </select>
    <insert id="insert">
        insert into user (name ,password) values(#{name },#{password})
    </insert>
    <select id="findUser" resultType="com.ryan.springbootvue.entity.User">
        select * from user where name=#{name} and password=#{password}
    </select>

</mapper>